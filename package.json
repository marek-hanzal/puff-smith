{
	"name": "puff-smith",
	"version": "0.6.18",
	"private": true,
	"scripts": {
		"next:dev": "next dev",
		"dev": "npm run next:dev",
		"build": "next build",
		"analyze": "cross-env ANALYZE=true next build",
		"migration": "node --no-warnings --loader ts-node/esm -r ts-node/register src/cli/migration.ts",
		"prisma migrate dev": "npx dotenv -c development npx prisma migrate dev --create-only",
		"prisma migrate reset": "npx dotenv -c development npx prisma migrate reset --force",
		"prisma generate": "npx dotenv -c development npx prisma generate",
		"prisma db push": "npx dotenv -c development npx prisma db push",
		"prisma format": "npx dotenv -c development npx prisma format",
		"start": "next start",
		"start:local": "concurrently 'npm:start'",
		"lint": "next lint",
		"next-unused": "next-unused"
	},
	"browserslist": [
		">0.3%",
		"not ie 11",
		"not dead",
		"not op_mini all"
	],
	"engines": {
		"node": ">=16.0.0"
	},
	"sideEffects": [
		"src/service/side-effect/*"
	],
	"next-unused": {
		"alias": {
			"@/puff-smith/": "src/",
			"@/sdk/": "src/sdk/"
		},
		"include": [
			"src"
		],
		"exclude": [],
		"entrypoints": [
			"src/pages"
		]
	},
	"dependencies": {
		"@ant-design/icons": "^4.7.0",
		"@leight-core/api": "^0.6.101",
		"@leight-core/client": "^0.6.92",
		"@leight-core/server": "^0.6.125",
		"@leight-core/utils": "^0.5.32",
		"@next-auth/prisma-adapter": "^1.0.3",
		"@prisma/client": "^4.1.0",
		"antd": "^4.21.7",
		"antd-mobile": "^5.19.0",
		"async-lock": "^1.3.2",
		"boolean": "^3.2.0",
		"delay": "^5.0.0",
		"js-file-download": "^0.4.12",
		"kothing-editor": "^0.1.4",
		"lru-cache": "^7.13.1",
		"matcher": "^5.0.0",
		"measure-time": "^5.0.8",
		"memoizee": "^0.4.15",
		"next": "^12.2.2",
		"next-auth": "^4.10.1",
		"next-compose-plugins": "^2.2.1",
		"next-global-css": "^1.3.1",
		"next-images": "^1.8.4",
		"nodemailer": "^6.7.7",
		"nprogress": "^0.2.0",
		"p-queue": "^7.2.0",
		"pretty-print-json": "^1.2.5",
		"prom-client": "^14.0.1",
		"randomatic": "^3.1.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.4.0",
		"sharp": "^0.30.7",
		"stream-chain": "^2.2.5",
		"stream-json": "^1.7.4",
		"ts-node": "^10.9.1",
		"typescript": "^4.7.4",
		"umzug": "^3.1.1",
		"unique-objects": "^1.0.5",
		"uuid": "^8.3.2",
		"winston": "^3.8.1",
		"winston-loki": "^6.0.5",
		"xlsx": "^0.18.5",
		"yaml": "^2.1.1"
	},
	"devDependencies": {
		"@types/async-lock": "^1.1.5",
		"@types/memoizee": "^0.4.8",
		"@types/node": "^18.0.6",
		"@types/nprogress": "^0.2.0",
		"@types/randomatic": "^3.1.3",
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"@types/stream-chain": "^2.0.1",
		"@types/stream-json": "^1.7.2",
		"@types/uuid": "^8.3.4",
		"concurrently": "^7.3.0",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^6.0.0",
		"eslint": "^8.20.0",
		"eslint-config-next": "^12.2.2",
		"next-transpile-modules": "^9.0.0",
		"next-unused": "^0.0.6",
		"path": "^0.12.7",
		"prisma": "^4.1.0",
		"typescript": "^4.7.4",
		"webpack-merge": "^5.8.0rea"
	}
}
