{
	"name": "puff-smith",
	"version": "0.5.126",
	"private": true,
	"scripts": {
		"next:dev": "next dev",
		"agenda": "npx dotenv -c development node ./.next/server/agenda.js",
		"migration": "npx dotenv -c development node ./.next/server/migration.js",
		"dev": "concurrently 'npm:next:dev' 'npm:agenda'",
		"build": "next build",
		"analyze": "cross-env ANALYZE=true next build",
		"prisma migrate dev": "npx dotenv -c development npx prisma migrate dev",
		"prisma migrate reset": "npx dotenv -c development npx prisma migrate reset --force",
		"prisma generate": "npx dotenv -c development npx prisma generate",
		"prisma db push": "npx dotenv -c development npx prisma db push",
		"prisma format": "npx dotenv -c development npx prisma format",
		"start": "next start",
		"start:local": "concurrently 'npm:start' 'npm:agenda'",
		"lint": "next lint"
	},
	"prisma": {
		"seed": "node ./.next/server/migration.js"
	},
	"sideEffects": [
		"src/service/*"
	],
	"dependencies": {
		"@ant-design/icons": "^4.7.0",
		"@leight-core/api": "^0.5.141",
		"@leight-core/client": "^0.5.98",
		"@leight-core/server": "^0.5.135",
		"@next-auth/prisma-adapter": "^1.0.3",
		"@prisma/client": "^3.10.0",
		"agenda": "^4.2.1",
		"antd": "^4.19.3",
		"boolean": "^3.2.0",
		"concurrently": "^7.0.0",
		"js-file-download": "^0.4.12",
		"matcher": "^5.0.0",
		"measure-time": "^5.0.7",
		"next": "^12.1.0",
		"next-auth": "^4.3.0",
		"next-compose-plugins": "^2.2.1",
		"next-global-css": "^1.3.0",
		"next-images": "^1.8.4",
		"nodemailer": "^6.7.3",
		"nprogress": "^0.2.0",
		"pretty-print-json": "^1.2.3",
		"prom-client": "^14.0.1",
		"randomatic": "^3.1.1",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"typescript": "^4.6.2",
		"uuid": "^8.3.2",
		"winston": "^3.7.2",
		"winston-loki": "^6.0.5",
		"xlsx": "^0.18.3"
	},
	"devDependencies": {
		"@types/is-empty": "^1.2.1",
		"@types/node": "^17.0.21",
		"@types/nprogress": "^0.2.0",
		"@types/randomatic": "^3.1.3",
		"@types/react": "^17.0.43",
		"@types/react-dom": "17.0.14",
		"@types/uuid": "^8.3.4",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^5.0.0",
		"eslint": "^8.11.0",
		"eslint-config-next": "^12.1.0",
		"next-transpile-modules": "^9.0.0",
		"path": "^0.12.7",
		"prisma": "^3.10.0",
		"typescript": "^4.6.2",
		"webpack-merge": "^5.8.0"
	}
}
