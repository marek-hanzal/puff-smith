{
	"name": "puff-smith",
	"version": "0.5.242",
	"private": true,
	"scripts": {
		"next:dev": "next dev",
		"dev": "npm run next:dev",
		"build": "next build",
		"analyze": "cross-env ANALYZE=true next build",
		"prisma migrate dev": "npx dotenv -c development npx prisma migrate dev",
		"prisma migrate reset": "npx dotenv -c development npx prisma migrate reset --force",
		"prisma generate": "npx dotenv -c development npx prisma generate",
		"prisma db push": "npx dotenv -c development npx prisma db push",
		"prisma format": "npx dotenv -c development npx prisma format",
		"start": "next start",
		"start:local": "concurrently 'npm:start'",
		"lint": "next lint",
		"next-unused": "next-unused"
	},
	"sideEffects": [
		"src/service/side-effect/*"
	],
	"next-unused": {
		"alias": {
			"@/puff-smith/": "src/",
			"@/sdk/": "src/sdk/"
		},
		"include": [
			"src"
		],
		"exclude": [],
		"entrypoints": [
			"src/pages"
		]
	},
	"dependencies": {
		"@ant-design/icons": "^4.7.0",
		"@leight-core/api": "^0.5.182",
		"@leight-core/client": "^0.5.159",
		"@leight-core/server": "^0.5.169",
		"@next-auth/prisma-adapter": "^1.0.3",
		"@prisma/client": "^3.13.0",
		"antd": "^4.20.3",
		"async-lock": "^1.3.1",
		"boolean": "^3.2.0",
		"js-file-download": "^0.4.12",
		"lru-cache": "^7.9.0",
		"matcher": "^5.0.0",
		"measure-time": "^5.0.8",
		"next": "^12.1.6",
		"next-auth": "^4.3.4",
		"next-compose-plugins": "^2.2.1",
		"next-global-css": "^1.3.1",
		"next-images": "^1.8.4",
		"nodemailer": "^6.7.4",
		"nprogress": "^0.2.0",
		"pretty-print-json": "^1.2.4",
		"prom-client": "^14.0.1",
		"randomatic": "^3.1.1",
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"sharp": "^0.30.4",
		"typescript": "^4.6.4",
		"unique-objects": "^1.0.5",
		"uuid": "^8.3.2",
		"winston": "^3.7.2",
		"winston-loki": "^6.0.5",
		"xlsx": "^0.18.5",
		"yaml": "^2.0.1",
		"p-queue": "^7.2.0",
		"stream-json": "^1.7.4",
		"stream-chain": "^2.2.5",
		"memoizee": "^0.4.15"
	},
	"devDependencies": {
		"@types/async-lock": "^1.1.5",
		"@types/is-empty": "^1.2.1",
		"@types/node": "^17.0.31",
		"@types/nprogress": "^0.2.0",
		"@types/randomatic": "^3.1.3",
		"@types/react": "^18.0.8",
		"@types/react-dom": "^18.0.3",
		"@types/uuid": "^8.3.4",
		"@types/stream-json": "^1.7.2",
		"@types/stream-chain": "^2.0.1",
		"@types/memoizee": "^0.4.8",
		"concurrently": "^7.1.0",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^5.1.0",
		"eslint": "^8.14.0",
		"eslint-config-next": "^12.1.5",
		"next-transpile-modules": "^9.0.0",
		"next-unused": "^0.0.6",
		"path": "^0.12.7",
		"prisma": "^3.13.0",
		"typescript": "^4.6.4",
		"webpack-merge": "^5.8.0"
	}
