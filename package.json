{
	"name": "puff-smith",
	"version": "0.7.0",
	"private": true,
	"scripts": {
		"next:dev": "next dev",
		"dev": "npm run next:dev",
		"build": "next build",
		"analyze": "cross-env ANALYZE=true next build",
		"migration": "node --no-warnings --loader ts-node/esm -r ts-node/register src/cli/migration.ts",
		"prisma migrate dev": "npx dotenv -c development npx prisma migrate dev --create-only",
		"prisma migrate reset": "npx dotenv -c development npx prisma migrate reset --force",
		"prisma generate": "npx dotenv -c development npx prisma generate",
		"prisma db push": "npx dotenv -c development npx prisma db push",
		"prisma format": "npx dotenv -c development npx prisma format",
		"start": "next start",
		"start:local": "concurrently 'npm:start'",
		"lint": "next lint",
		"next-unused": "next-unused"
	},
	"browserslist": [
		">0.3%",
		"not ie 11",
		"not dead",
		"not op_mini all"
	],
	"engines": {
		"node": ">=16.0.0"
	},
	"sideEffects": [
		"src/service/side-effect/*"
	],
	"next-unused": {
		"alias": {
			"@/puff-smith/": "src/",
			"@/sdk/": "src/sdk/"
		},
		"include": [
			"src"
		],
		"exclude": [],
		"entrypoints": [
			"src/pages"
		]
	},
	"dependencies": {
		"@leight-core/viv": "^0.5.0-experimental1",
		"@ant-design/icons": "^4.7.0",
		"@next-auth/prisma-adapter": "^1.0.4",
		"@prisma/client": "^4.4.0",
		"antd": "^4.23.5",
		"antd-mobile": "^5.24.0",
		"delay": "^5.0.0",
		"fs-extra": "^10.1.0",
		"glob": "^8.0.3",
		"humanize-duration": "^3.27.3",
		"is-callable": "^1.2.7",
		"is-empty": "^1.2.0",
		"is-string": "^1.0.7",
		"js-file-download": "^0.4.12",
		"kothing-editor": "^0.1.4",
		"lru-cache": "^7.14.0",
		"measure-time": "^5.0.8",
		"memoizee": "^0.4.15",
		"micromatch": "^4.0.5",
		"mime-types": "^2.1.35",
		"next": "^12.3.1",
		"next-auth": "^4.13.0",
		"next-compose-plugins": "^2.2.1",
		"next-global-css": "^1.3.1",
		"next-images": "^1.8.4",
		"nodemailer": "^6.7.8",
		"nprogress": "^0.2.0",
		"p-queue": "^7.3.0",
		"pretty-print-json": "^1.2.6",
		"randomatic": "^3.1.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.4.0",
		"sharp": "^0.31.1",
		"tar": "^6.1.11",
		"touch": "^3.1.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.4",
		"umzug": "^3.2.1",
		"uuid": "^9.0.0",
		"winston": "^3.8.2",
		"winston-loki": "^6.0.6",
		"xlsx": "^0.18.5",
		"yaml": "^2.1.1"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.13",
		"@types/glob": "^8.0.0",
		"@types/humanize-duration": "^3.27.1",
		"@types/is-callable": "^1.1.0",
		"@types/is-empty": "^1.2.1",
		"@types/is-string": "^1.0.0",
		"@types/memoizee": "^0.4.8",
		"@types/micromatch": "^4.0.2",
		"@types/mime-types": "^2.1.1",
		"@types/node": "^18.7.16",
		"@types/nprogress": "^0.2.0",
		"@types/randomatic": "^3.1.3",
		"@types/react": "^18.0.21",
		"@types/react-dom": "^18.0.6",
		"@types/tar": "^6.1.3",
		"@types/touch": "^3.1.2",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.40.0",
		"@typescript-eslint/parser": "^5.40.0",
		"concurrently": "^7.4.0",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^6.0.0",
		"eslint": "^8.25.0",
		"eslint-config-next": "^12.3.1",
		"eslint-plugin-unicorn": "^44.0.2",
		"next-transpile-modules": "^9.0.0",
		"next-unused": "^0.0.6",
		"path": "^0.12.7",
		"prisma": "^4.4.0",
		"typescript": "^4.8.4",
		"webpack-merge": "^5.8.0"
	}
}
