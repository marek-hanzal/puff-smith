{
    "name": "puff-smith",
    "version": "0.7.0",
    "private": true,
    "scripts": {
        "next:dev": "next dev",
        "dev": "npm run next:dev",
        "build": "next build",
        "analyze": "cross-env ANALYZE=true next build",
        "migration": "node --no-warnings --loader ts-node/esm -r ts-node/register src/cli/migration.ts",
        "prisma migrate dev": "npx dotenv -c development npx prisma migrate dev --create-only",
        "prisma migrate reset": "npx dotenv -c development npx prisma migrate reset --force",
        "prisma generate": "npx dotenv -c development npx prisma generate",
        "prisma db push": "npx dotenv -c development npx prisma db push",
        "prisma format": "npx dotenv -c development npx prisma format",
        "start": "next start",
        "lint": "next lint"
    },
    "browserslist": [
        ">0.3%",
        "not ie 11",
        "not dead",
        "not op_mini all"
    ],
    "engines": {
        "node": ">=16.0.0"
    },
    "dependencies": {
        "@ant-design/icons": "^4.7.0",
        "@leight/antd": "*",
        "@leight/logger": "*",
        "@leight/prisma": "*",
        "@leight/shared": "*",
        "@leight/ui": "*",
        "@leight/utils": "*",
        "@prisma/client": "^4.4.0",
        "@tanstack/react-query": "^4.13.0",
        "antd": "^4.23.6",
        "antd-mobile": "^5.24.1",
        "i18next": "^22.0.3",
        "next": "^13.0.0",
        "nodemailer": "^6.7.8",
        "nprogress": "^0.2.0",
        "pretty-print-json": "^1.2.6",
        "randomatic": "^3.1.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^12.0.0",
        "react-icons": "^4.4.0",
        "umzug": "^3.2.1",
        "uuid": "^9.0.0",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@types/node": "^18.7.16",
        "@types/nprogress": "^0.2.0",
        "@types/randomatic": "^3.1.3",
        "@types/react": "^18.0.21",
        "@types/react-dom": "^18.0.6",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "cross-env": "^7.0.3",
        "dotenv-cli": "^6.0.0",
        "eslint": "^8.25.0",
        "eslint-config-next": "^12.3.1",
        "eslint-config-turbo": "^0.0.4",
        "eslint-plugin-unicorn": "^44.0.2",
        "next-transpile-modules": "^9.0.0",
        "prisma": "^4.4.0",
        "typescript": "^4.8.4",
        "webpack": "^5.74.0",
        "webpack-merge": "^5.8.0"
    }
