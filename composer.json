{
	"name": "marek-hanzal/puff-smith",
	"description": "Vape Stuff for Vapers.",
	"config": {
		"bin-compat": "full",
		"discard-changes": true,
		"allow-plugins": {
			"composer/package-versions-deprecated": true
		}
	},
	"autoload": {
		"psr-4": {
			"": [
				"./src"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"": [
				"./test"
			]
		}
	},
	"require": {
		"ext-memcached": "*",
		"leight-core/edde": "dev-main",
		"mpdf/mpdf": "^8.0",
		"setasign/fpdi": "^2.3",
		"setasign/fpdf": "^1.8",
		"setasign/tfpdf": "^1.32",
		"phpunit/phpunit": "^8",
		"cache/memcached-adapter": "^1.2.0"
	}
}
